{
  "manifest_version": 3,
  "name": "HelpMeOut",
  "version": "1.0.1",
  "action": {
    "default_popup":"index.html"
  },
  "permissions": [
    "scripting",
    "tabs",
    "activeTab"
  ],
  "background": {
    "service_worker": "service-worker.js",
      "type": "module"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "32": "icon32.png"
  }
}